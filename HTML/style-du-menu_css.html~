<span class="normal">		</span><span class="selector">#menu</span><span class="normal"> li</span><span class="symbol">:</span><span class="normal">hover </span><span class="symbol">&gt;</span><span class="normal"> </span><span class="selector">.sous</span><span class="symbol">-</span><span class="normal">menu</span><span class="cbracket">{</span>
